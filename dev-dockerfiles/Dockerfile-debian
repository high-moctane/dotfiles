FROM debian:buster

RUN apt-get update \
    && apt-get install -y curl make git
RUN curl -L mocdot.ml | make apt -f - PROTOCOL=https
RUN curl -L mocdot.ml | make brew-setup -f - PROTOCOL=https
# RUN curl -L mocdot.ml | make brew-install -f - PROTOCOL=https
# RUN curl -L mocdot.ml | make all -f - PROTOCOL=https

WORKDIR /root

CMD ["/bin/bash"]
